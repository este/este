{
  "name": "este",
  "private": true,
  "engines": {
    "node": "8.x.x"
  },
  "license": "MIT",
  "scripts": {
    "api": "nodemon --config nodemon.api.json",
    "web": "nodemon --config nodemon.web.json",
    "schema": "yarn graphql get-schema -p app -o server/schema.graphql",
    "relay": "relay-compiler --include components/** mutations/** pages/** --src ./ --schema server/schema.graphql",
    "schema-relay": "yarn schema && yarn relay",
    "relay:watch": "yarn relay --watch",
    "playground": "graphql playground --port 5000",
    "dev": "run-p api web relay:watch playground",
    "env": "node ./scripts/env",
    "build": "next build",
    "production:api": "cross-env NODE_ENV=production node server/api.js",
    "production:web": "cross-env NODE_ENV=production node server/web.js",
    "production": "yarn clean:babel:cache && env-cmd .env npm-run-all build --parallel production:api production:web",
    "start": "yarn $DEPLOY_TARGET",
    "deploy:db": "yarn prisma deploy",
    "deploy:api": "now -e DEPLOY_TARGET=production:api --dotenv .env.production",
    "deploy:web": "now -e DEPLOY_TARGET=production:web --dotenv .env.production",
    "extract:messages": "yarn clean-babel-cache && rimraf lang/.messages && next build && node ./scripts/extractMessages",
    "favicon": "node ./scripts/favicon",
    "flow": "flow",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prettier": "prettier --write '**/*.js'",
    "clean:babel:cache": "rimraf node_modules/.cache/babel-loader",
    "relay:ci": "relay-compiler --watchman false --schema server/schema.graphql --src . --include components/** --include mutations/** --include pages/**",
    "test": "run-s flow jest relay:ci build",
    "deps": "rimraf node_modules yarn.lock flow-typed && yarn && flow-typed install",
    "analyze": "cross-env ANALYZE=1 next build"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "ajv": "^6.0.0-rc.1",
    "array-equal": "^1.0.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "bcryptjs": "^2.4.3",
    "big.js": "^5.0.3",
    "color": "^3.0.0",
    "cookie": "^0.3.1",
    "create-react-context": "^0.1.3",
    "cross-env": "^5.0.1",
    "diacritics-map": "^0.1.0",
    "draft-js": "^0.10.4",
    "glob": "^7.1.2",
    "graphql": "^0.13.1",
    "graphql-yoga": "^1.3.2",
    "gravatar": "^1.6.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.1",
    "next": "5.0.1-canary.7",
    "nprogress": "^0.2.0",
    "path-match": "^1.2.4",
    "prisma-binding": "^1.5.12",
    "prop-types": "^15.6.0",
    "raf": "^3.3.2",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.3.0",
    "react-relay": "^1.5.0",
    "relay-runtime": "^1.5.0",
    "resize-observer-polyfill": "^1.5.0",
    "smoothscroll-polyfill": "^0.4.0",
    "url": "^0.11.0",
    "validator": "^9.4.0",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.0.1",
    "babel-plugin-relay": "^1.5.0",
    "dotenv": "^5.0.0",
    "env-cmd": "^7.0.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.1.5",
    "graphql-cli": "^2.13.0",
    "jest": "^22.0.1",
    "nodemon": "^1.15.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.5.1",
    "prisma": "^1.2.4",
    "react-test-renderer": "^16.0.0",
    "relay-compiler": "^1.5.0",
    "relay-devtools": "^1.4.0",
    "rimraf": "^2.4.5"
  }
}
